server {
  domain: "google.com";
  scheme: "https";
  port: "443";

  location "/api/router" {
    match $path "^/api/(.*)$";
    parse $query_string;
    var $sb "100";
    return "https://h5.ele.me$path$path/$#foo/$1$1$1/$0*$0*$0$sb";
  }
  location "/shop/?" {
    return "https://h5.ele.me";
  }
}

server {
  domain: "ele.me";
  scheme: "https";
  port: "443";

  location "/" {
    if (equal $scheme "https") {
      return "https://help.ele.me";
    }
    return "https://h5.ele.me";
  }
